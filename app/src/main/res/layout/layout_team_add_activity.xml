<?xml version="1.0" encoding="utf-8"?>
<!--添加车队账号界面-->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/text_mid"
            android:textColor="@color/colorPrimary"
            android:drawableLeft="@drawable/icon_star_red"
            android:drawableStart="@drawable/icon_star_red"
            android:text="@string/team_code"
            android:gravity="center"/>
        <EditText
            android:id="@+id/team_add_code"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:background="@drawable/bg_edit"
            android:textColor="@color/text_main"
            android:textSize="@dimen/text_small"
            android:gravity="center_vertical"
            tools:text="123456"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:textSize="@dimen/text_mid"
            android:textColor="@color/colorPrimary"
            android:drawableLeft="@drawable/icon_star_red"
            android:drawableStart="@drawable/icon_star_red"
            android:text="@string/team_name"
            android:gravity="center"/>
        <EditText
            android:id="@+id/team_add_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:background="@drawable/bg_edit"
            android:textColor="@color/text_main"
            android:textSize="@dimen/text_small"
            android:gravity="center_vertical"
            tools:text="123456"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/team_add_view_account"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="5dp"
